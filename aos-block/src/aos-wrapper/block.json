{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "fwd-blocks/aos-wrapper",
    "title": "AOS Wrapper",
    "category": "design",
    "icon": "smiley",
    "description": "A wrapper block to animate nested blocks using AOS.",
    "supports": {
        "html": false,
        "align": true,
        "innerBlocks": true
    },
    "attributes": {
        "animationType": {
            "type": "string",
            "default": "fade-up"
        }
    },
    "textdomain": "aos-wrapper",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}